{
  "compilerOptions": {
    "module": "commonjs",
    "removeComments": true,
    "noUnusedParameters": true,
    "noUnusedLocals": false,
    "target": "es6",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./src"
  },
  "include": ["src/**/*.ts"],
  "exclude": [
    "node_modules",
    "**/*.spec.ts"
  ]
}



